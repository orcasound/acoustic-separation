{
    "train_csv": "dataset/orca_train.csv",
    "validation_csv": "dataset/orca_validation.csv",
    "model_dir": "2stems-finetune",
    "mix_name": "mix",
    "instrument_list": ["orca_vocals", "noise"],
    "sample_rate":44100,
