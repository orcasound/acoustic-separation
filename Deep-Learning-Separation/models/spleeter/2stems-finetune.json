    "learning_rate": 1e-4,
    "batch_size":4,
    "training_cache":"cache/training_cache",
    "validation_cache":"cache/validation_cache",
    "train_max_steps": 2000000,
    "throttle_secs":600,
    "random_seed":3,
    "save_checkpoints_steps":300,
    "save_summary_steps":5,
    "model":{
        "type":"unet.unet",
        "params":{
               "conv_activation":"ELU",
               "deconv_activation":"ELU"
        }
    }
